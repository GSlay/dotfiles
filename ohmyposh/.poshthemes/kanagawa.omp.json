{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "background": "#1F1F28",
    "background_highlight": "#2A2A37",
    "foreground": "#DCD7BA",
    "blue": "#7E9CD8",
    "green": "#98BB6C",
    "yellow": "#E6C384",
    "red": "#C34043",
    "aqua": "#7AA89F",
    "violet": "#957FB8"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:background_highlight",
          "foreground": "p:foreground",
          "properties": {
            "arch": "\uf303",
            "debian": "\uf306",
            "fedora": "\uf30a",
            "linux": "\uf17c",
            "macos": "\uf179",
            "manjaro": "\uf312",
            "opensuse": "\uf314",
            "ubuntu": "\uf31b",
            "windows": "\uf17a"
          },
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:blue",
          "foreground": "p:foreground",
          "style": "plain",
          "properties": {
            "folder_icon": "...",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:violet",
          "foreground": "p:foreground",
          "style": "plain",
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:aqua",
          "foreground": "p:background",
          "style": "plain",
          "properties": {
            "always_enabled": true
          },
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "p:red",
          "foreground": "p:foreground",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:yellow{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \uf08a{{ if gt .Code 0 }} {{.Code}}{{ end }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:background_highlight",
          "foreground": "p:yellow",
          "style": "plain",
          "properties": {
            "fetch_version": false
          },
          "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "p:background_highlight",
          "foreground": "p:blue",
          "style": "plain",
          "properties": {
            "fetch_version": false
          },
          "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "p:background_highlight",
          "foreground": "p:green",
          "style": "plain",
          "properties": {
            "fetch_version": false
          },
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "p:background_highlight",
          "foreground": "p:red",
          "style": "plain",
          "properties": {
            "fetch_version": false
          },
          "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "background": "p:background_highlight",
          "foreground": "p:red",
          "style": "plain",
          "properties": {
            "fetch_version": false
          },
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "java"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:background_highlight",
          "foreground": "p:foreground",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} - {{ .Folder }}",
  "final_space": true,
  "version": 3
}
